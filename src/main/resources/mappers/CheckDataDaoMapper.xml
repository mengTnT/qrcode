<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.tthome.qrcode.dao.CheckDataDao">

    <resultMap id="BaseResultMap" type="com.tthome.qrcode.entity.CheckData">
        <result column="check_data_id" property="id"/>
        <result column="check_data_hashcode" property="hashcode"/>
        <result column="is_check" property="isCheck"/>

    </resultMap>

   <insert id="insert">
       INSERT INTO check_data VALUES(0,#{hashcode},#{isCheck})
   </insert>

</mapper>